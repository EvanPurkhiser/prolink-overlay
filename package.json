{
  "name": "prolink-tools",
  "version": "0.1.0-beta.5",
  "description": "A collection of userfriendly tools for DJs using Pioneers PRO DJ LINK feature",
  "author": "Evan Purkhiser <evanpurkhiser@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start-dev:main": "webpack --config=webpack.config.main.ts",
    "start-dev:renderer": "webpack --config=webpack.config.renderer.ts",
    "start-dev:website": "webpack --config=webpack.config.website.ts",
    "start-dev:api": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/api/index.ts",
    "start-dev": "run-p start-dev:*",
    "start": "electron dist/main.js",
    "start-api": "ts-node ./src/api/index.ts",
    "build-main": "cross-env NODE_ENV=production webpack --config=webpack.config.main.ts",
    "build-renderer": "cross-env NODE_ENV=production webpack --config=webpack.config.renderer.ts",
    "build-website": "cross-env NODE_ENV=production webpack --config=webpack.config.website.ts",
    "vercel-build-website": "git fetch --tags && yarn build-website",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "lint": "eslint src/"
  },
  "main": "main.js",
  "build": {
    "productName": "Prolink Tools",
    "appId": "com.evanpurkhiser.tools.prolink",
    "artifactName": "${name}-${version}-${os}.${ext}",
    "directories": {
      "output": "release"
    },
    "publish": null,
    "files": [
      "!**/*",
      {
        "from": "dist/",
        "to": "./",
        "filter": [
          "**/*",
          "!**/*.map"
        ]
      },
      "node_modules/better-sqlite3/**/*",
      "node_modules/bindings/**/*",
      "node_modules/file-uri-to-path/**/*",
      "package.json"
    ],
    "extraFiles": [
      "LICENSE"
    ],
    "npmRebuild": false,
    "linux": {
      "target": "tar.gz"
    },
    "win": {
      "target": "portable"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.music"
    }
  },
  "resolutions": {
    "cursor": "https://github.com/EvanPurkhiser/node-cursor.git#fix-buffer"
  },
  "devDependencies": {
    "@evanpurkhiser/eslint-config": "^0.9.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0-beta.0",
    "@types/webpack-plugin-serve": "^1.2.0",
    "electron": "^11.2.0",
    "electron-builder": "^22.3.2",
    "eslint": "^7.14.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.0",
    "prettier": "^2.0.5",
    "typescript-styled-plugin": "^0.15.0",
    "webpack-cli": "^4.3.0",
    "webpack-plugin-serve": "^1.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
