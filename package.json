    "@octokit/request": "^5.4.5",
    "@octokit/types": "^6.1.2",
    "@popperjs/core": "^2.6.0",
    "@sentry/apm": "^5.16.1",
    "@sentry/browser": "^6.0.1",
    "@sentry/electron": "^2.1.0",
    "@sentry/node": "^6.0.1",
    "@sentry/types": "^6.0.1",
    "@types/connect": "^3.4.33",
    "@types/cookie": "^0.4.0",
    "@types/deep-diff": "^1.0.0",
    "@types/electron-settings": "^3.1.1",
    "@types/http-proxy": "^1.17.4",
    "@types/jest": "^26.0.13",
    "@types/koa": "^2.11.6",
